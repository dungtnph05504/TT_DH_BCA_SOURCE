<jxb:bindings xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
    jxb:extensionBindingPrefixes="xjc"
    jxb:version="2.0">
	<jxb:globalBindings generateElementProperty="false">
		<xjc:javaType name="java.util.Date" xmlType="xs:date" adapter="com.nec.asia.nic.dx.utils.DateAdapter" />
		<xjc:javaType name="java.util.Date" xmlType="xs:dateTime" adapter="com.nec.asia.nic.dx.utils.DateTimeAdapter" />
	</jxb:globalBindings>
</jxb:bindings>
