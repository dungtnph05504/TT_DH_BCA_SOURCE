<jxb:bindings
	version="1.0"
	xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
	xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
	jxb:extensionBindingPrefixes="xjc"
	xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<jxb:bindings
		schemaLocation="persoStatus.xsd"
		node="/xs:schema">
		<jxb:globalBindings>
			<xjc:simple />
			<xjc:javaType
				name="java.util.Date"
				xmlType="xs:dateTime"
				adapter="com.nec.asia.nic.dx.utils.DateTimeAdapter" />
			<xjc:javaType
				name="java.util.Date"
				xmlType="xs:date"
				adapter="com.nec.asia.nic.dx.utils.DateAdapter" />
		</jxb:globalBindings>
	</jxb:bindings>
</jxb:bindings>