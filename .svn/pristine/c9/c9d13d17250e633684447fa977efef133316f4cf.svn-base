package com.nec.asia.nic.comp.trans.domain;

import java.io.Serializable;
import java.util.Date;

public class NicTransactionPaymentDetail implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	
	public static final String NODE_PAYMENT_DESC = "Thêm phí bổ sung.";
	public static final String NODE_PAYMENT_DESC_PC = "Thay đổi phí chính do sai loại hồ sơ.";
	
	private Long id;
	private String paymentId;
	private String typePayment;
	private boolean statusFee;
	private String subTypePayment;
	private Double paymentAmount;
	private Date createDate;
	private String createBy;
	private String createWsktnId;
	private String updateBy;
	private String updateWsktnId;
	private Date updateDate;
	private String note;
	
	public NicTransactionPaymentDetail() {
		super();
	}
	public NicTransactionPaymentDetail(Long id, String paymentId,
			String typePayment, boolean statusFee, String subTypePayment,
			Double paymentAmount, Date createDate, String createBy,
			String createWsktnId, String updateBy, String updateWsktnId,
			Date updateDate, String note) {
		super();
		this.id = id;
		this.paymentId = paymentId;
		this.typePayment = typePayment;
		this.statusFee = statusFee;
		this.subTypePayment = subTypePayment;
		this.paymentAmount = paymentAmount;
		this.createDate = createDate;
		this.createBy = createBy;
		this.createWsktnId = createWsktnId;
		this.updateBy = updateBy;
		this.updateWsktnId = updateWsktnId;
		this.updateDate = updateDate;
		this.note = note;
	}
	
	
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public String getPaymentId() {
		return paymentId;
	}
	public void setPaymentId(String paymentId) {
		this.paymentId = paymentId;
	}
	public String getTypePayment() {
		return typePayment;
	}
	public void setTypePayment(String typePayment) {
		this.typePayment = typePayment;
	}
	public boolean isStatusFee() {
		return statusFee;
	}
	public void setStatusFee(boolean statusFee) {
		this.statusFee = statusFee;
	}
	public String getSubTypePayment() {
		return subTypePayment;
	}
	public void setSubTypePayment(String subTypePayment) {
		this.subTypePayment = subTypePayment;
	}
	public Double getPaymentAmount() {
		return paymentAmount;
	}
	public void setPaymentAmount(Double paymentAmount) {
		this.paymentAmount = paymentAmount;
	}
	public Date getCreateDate() {
		return createDate;
	}
	public void setCreateDate(Date createDate) {
		this.createDate = createDate;
	}
	public String getCreateBy() {
		return createBy;
	}
	public void setCreateBy(String createBy) {
		this.createBy = createBy;
	}
	public String getCreateWsktnId() {
		return createWsktnId;
	}
	public void setCreateWsktnId(String createWsktnId) {
		this.createWsktnId = createWsktnId;
	}
	public String getUpdateBy() {
		return updateBy;
	}
	public void setUpdateBy(String updateBy) {
		this.updateBy = updateBy;
	}
	public String getUpdateWsktnId() {
		return updateWsktnId;
	}
	public void setUpdateWsktnId(String updateWsktnId) {
		this.updateWsktnId = updateWsktnId;
	}
	public Date getUpdateDate() {
		return updateDate;
	}
	public void setUpdateDate(Date updateDate) {
		this.updateDate = updateDate;
	}
	public String getNote() {
		return note;
	}
	public void setNote(String note) {
		this.note = note;
	}
	
	

}
