body {
    background: #FFFFC6;
    background-position: top center;
    background-attachment: fixed;
}
.m_top_15{
	margin-top:15px;
}
.link {
    padding: 20px 0px;
    text-align: center;
    clear: both;
}

    .link a {
        color: rgb(51, 51, 51);
    }

.wrapper {
    margin: 50px auto;
    width: 960px;
}

.form1 {
    background: rgb(255, 255, 255);
    border-radius: 0px 0px 6px 6px;
    width: 450px;
    color: rgb(119, 119, 119);
    overflow: hidden;
    float: left;
    box-shadow: 0px 0px 8px 2px #d1d1d1;
    -webkit-box-shadow: 0px 0px 8px 2px #d1d1d1;
    -moz-box-shadow: 0px 0px 8px 2px #d1d1d1;
    -webkit-border-top-left-radius: 0px;
    -webkit-border-top-right-radius: 0px;
    -webkit-border-bottom-right-radius: 6px;
    -webkit-border-bottom-left-radius: 6px;
    -moz-border-radius-topleft: 0px;
    -moz-border-radius-topright: 0px;
    -moz-border-radius-bottomright: 6px;
    -moz-border-radius-bottomleft: 6px;
}

.formtitle {
    background: linear-gradient(rgb(197, 222, 234) 0%, rgb(138, 187, 215) 8%, rgb(6, 109, 171) 100%);
    padding: 10px;
    width: 430px;
    color: rgb(255, 255, 255);
    line-height: 16px;
    font-size: 20px;
    font-weight: bold;
}

.input {
    margin: 20px;
    width: 410px;
    overflow: hidden;
    border-bottom-color: rgb(221, 221, 221);
    border-bottom-width: 1px;
    border-bottom-style: solid;
}

.inputtext {
    width: 120px;
    height: 35px;
    line-height: 18px;
    font-size: 14px;
    float: left;
}

.inputcontent {
    width: 290px;
    height: 50px;
    float: left;
}

    .inputcontent input {
        padding: 5px;
        border-radius: 6px;
        border: 1px solid rgb(199, 199, 199);
        border-image: none;
        width: 200px;
        height: 18px;
        color: rgb(119, 119, 119);
        line-height: 18px;
        -moz-border-radius: 6px;
        -webkit-border-radius: 6px;
    }

    .inputcontent a {
        color: rgb(0, 132, 255);
        line-height: 12px;
        font-size: 12px;
        text-decoration: none;
    }

.nobottomborder {
    border-bottom-color: currentColor;
    border-bottom-width: 0px;
    border-bottom-style: none;
}

.buttons {
    background: rgb(241, 241, 241);
    padding: 15px;
    width: 430px;
    height: 34px;
    border-top-color: rgb(221, 221, 221);
    border-top-width: 1px;
    border-top-style: solid;
}

.greybutton {
    background: -ms-linear-gradient(rgb(225, 225, 225) 0%, rgb(186, 186, 186) 100%);
    padding: 6px 12px;
    border-radius: 20px;
    border: 1px solid rgb(186, 186, 186);
    border-image: none;
    color: rgb(255, 255, 255);
    line-height: 20px;
    font-size: 16px;
    margin-left: 10px;
    float: right;
    cursor: pointer;
    text-shadow: -1px -1px #bababa;
    -moz-border-radius: 20px;
    -webkit-border-radius: 20px;
}

    .greybutton:hover {
        background: -ms-linear-gradient(rgb(186, 186, 186) 0%, rgb(225, 225, 225) 100%);
    }

.orangebutton {
    padding: 6px 12px;
    border-radius: 10px;
    color: rgb(255, 255, 255);
    line-height: 20px;
    font-size: 16px;
    margin-left: 10px;
    float: right;
    cursor: pointer;
    background-image: -ms-linear-gradient(rgb(169, 3, 41), rgb(109, 0, 25));
    background-color: rgb(169, 3, 41);
    -moz-border-radius: 10px;
    -webkit-border-radius: 10px;
}

#heading {
    height: 45px;
    border-bottom-color: rgb(51, 51, 51);
    border-bottom-width: 1px;
    border-bottom-style: solid;
}

