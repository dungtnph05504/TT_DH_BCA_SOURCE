package com.nec.asia.nic.utils;

import java.util.List;

/**
 * @author aparna_sharma
 * 
 */
public class ListUtil {

	/**
	 * This method will return true if list is not null and not empty and size
	 * >=1
	 * 
	 * @param list
	 * @return
	 */
	public static boolean isNotNullAndNotEmpty(List<?> list) {
		if (list != null && !list.isEmpty() && list.size() > 0) {
			return true;
		}
		return false;

	}

}
