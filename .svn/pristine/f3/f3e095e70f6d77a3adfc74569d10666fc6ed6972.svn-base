.customizedDisplayTag {
	border: 1px solid #CCCCCC;
	width: 100%;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 9pt;
	width: 100%;
}

.customizedDisplayTag th {
	text-align: center;
	background: #F4F9FE url(images/background.gif);
	background-color: #F4F9FE;
	color: black;
	background-position: center center;
	background-repeat: no-repeat;
	cursor: default;
	height: 25px;
	padding-top: 0px;
	padding-bottom: 0px;
}
.customizedDisplayTag td.normal {
	background-color: whitesmoke;
	padding: 2px 4px 2px 4px;
}
.customizedDisplayTag tr.even {
	background-color: whitesmoke;
}

.customizedDisplayTag tr.odd {
	background-color: whitesmoke;
}


.customizedDisplayTag tr.even {
	height: 25px;
	background-color: #fff;
}

.customizedDisplayTag tr.odd {
	height: 25px;
	background-color: #fff;
}

.customizedDisplayTag td {
	padding: 4px;
	text-align: left;
	vertical-align: middle;
	padding-top: 0px;
	padding-bottom: 0px;
}
.displayTag {
	border: 1px solid #CCCCCC;
	width: 100%;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 9pt;
	width: 99.8%;
}

.displayTag th {
	color: white;
	background-color: #666699;
	text-align: left;
	padding: 2px 4px 2px 4px;
	white-space: nowrap;
}

.displayTag th.report {
	color: white;
	background-color: #666699;
	background-position: right;
	text-align: center;
	padding: 2px 4px 2px 4px;
}

.displayTag th.sorted a {
	background: no-repeat;
	background-position: right;
	display: block;
	width: 99.8%;
}

.displayTag th.sortable a {
	background: no-repeat;
	background-position: right;
	display: block;
	cursor: pointer;
	width: 99.8%;
}

th.sorted a, th.sortable a {
  padding-right: 4px;
}

.displayTag th.sortable a {
	background-image: url(images/arrow_off.png);
}

.displayTag th.order1 a {
	background-image: url(images/arrow_down.png);
}

.displayTag th.order2 a {
	background-image: url(images/arrow_up.png);
}

.displayTag tr.even {
	background-color: #eeeeee;
}

.displayTag tr.odd {
	background-color: whitesmoke;
}

.displayTag td {
	padding: 2px 4px 2px 4px;
	text-align: left;
	vertical-align: top;
}

.displayTag td.total {
	background-color: #E2E2E2;
	padding: 2px 4px 2px 4px;
}

.displayTag td.normal {
	background-color: whitesmoke;
	padding: 2px 4px 2px 4px;
}

.displayTag a {
	color: #F0E68C;
}

.displayTag tr.even a,.displayTag tr.odd a {
	color: #3366CC;
}

/******************************************************************************
* Highlight of display tag table when mouseover and on click
*******************************************************************************/
.displayTag tr.highlight {
	background: #9D9DFF;
}

.displayTag tr.selected {
	background: #CCCCFF;
	color: #fff;
}

.reportTable {
	border: 1px solid #A0A0A0;
	width: 100%;
}

.reportTable th {
	color: white;
	background-color: #666699;
	text-align: center;
	padding: 0px 0px 0px 0px;
}

.reportTable tr.even {
	background-color: #eeeeee;
}

.reportTable tr.odd {
	background-color: whitesmoke;
}

.reportTable td {
	padding: 0px 0px 0px 0px;
}

.reportTable td.total {
	background-color: #E2E2E2;
	padding: 0px 0px 0px 0px;
}

.reportTable td.normal {
	background-color: whitesmoke;
	padding: 0px 0px 0px 0px;
}

.reportTable a {
	color: #3366CC;
}

.reportTable tr.even a,.displayTag tr.odd a {
	color: #3366CC;
}

