div.menuBar,div.menuBar a.menuButton,div.menu,div.menu a.menuItem {
	font-family: Arial, "MS Sans Serif", sans-serif;
	font-size: 9pt;
	font-style: normal;
	font-weight: normal;
	color: #000000;
}

html,#contents {
	min-height: 100%;
	width: 100%;
	height: 100%;
}

html>body,html>body #contents {
	height: auto;
}

#contents {
	position: absolute;
	top: 0;
	left: 0;
