/******************************************************************************
* Defines default styles for site pages.                                      *
*******************************************************************************/
a {
	text-decoration: none;
}

a:hover {
	text-decoration: underline;
}
body {
	margin: 0;
	background-color: #F9F9F9;
	color: #000000;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 9pt;
	height: 100%;
}
.btn-search {
	height: 30px;
    width: 80px;	
}
h1 {
	font-size: 200%;
}

h2 {
	font-size: 140%;
}
.cla-font {
	
}
h3 {
	font-size: 120%;
}

h4 {
	font-size: 100%;
}

td {
	font-size: 9pt;
}

th {
	font-weight: bold;
	font-size: 9pt;
}

fieldset {
	font-size: 9pt;
}

input,textarea {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 11px;
	padding: 3px;
	border: 1px solid #d6d1d1;
}

select {
	font-family: Helvetica, Arial, sans-serif;
	font-size: 1em;
	padding: 3px;
	border: 1px solid #d6d1d1;
}

form {
	margin: 0;
}

table {
