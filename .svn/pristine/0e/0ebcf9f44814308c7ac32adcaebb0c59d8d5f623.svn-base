package com.nec.asia.nic.comp.trans.service.exception;

import com.nec.asia.nic.framework.exception.CoreException;

public class CardStatusUpdateServiceException extends CoreException{
	
	/**
	 * Instantiates a new CardStatusUpdateServiceException exception.
	 *
	 * @param message the message
	 */
	public CardStatusUpdateServiceException(String message) {
		super(message);
		
	}

	/**
	 * Instantiates a new CardStatusUpdateServiceException exception.
	 *
	 * @param cause the cause
	 */
	public CardStatusUpdateServiceException(Throwable cause) {
		super(cause);
		
	}

	/**
	 * Instantiates a new CardStatusUpdateServiceException exception.
	 *
	 * @param message the message
	 * @param cause the cause
	 */
	public CardStatusUpdateServiceException(String message, Throwable cause) {
		super(message, cause);
		
	}
	

}
