package com.nec.asia.nic.framework.job.scheduler.exception;

import com.nec.asia.nic.framework.exception.CoreException;


/**
 * The Class CreateScheduleException.
 *
 * @author boon_wui
 * @since 14 April 208
 * @version 1.0
 */
public class CreateScheduleException extends CoreException{

	/**
	 * The Constructor.
	 */
	public CreateScheduleException() {
		super("Failed to schedule job.");
	}

	/**
	 * The Constructor.
	 * 
	 * @param message
	 *            the message
	 */
	public CreateScheduleException(String message) {
		super(message);
	}

	/**
	 * The Constructor.
	 * 
	 * @param message
	 *            the message
	 * @param cause
	 *            the cause
	 */
	public CreateScheduleException(String message, Throwable cause) {
		super(message, cause);
	}

	/**
	 * The Constructor.
	 * 
	 * @param cause
	 *            the cause
	 */
	public CreateScheduleException(Throwable cause) {
		super("Failed to schedule job. Cause: "+cause.getMessage(),cause);
	}
}
