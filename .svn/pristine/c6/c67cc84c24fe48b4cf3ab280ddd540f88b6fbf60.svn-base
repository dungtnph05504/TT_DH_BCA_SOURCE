package com.nec.asia.nic.comp.trans.utils;


/**
 * The Class ReportDefinition.
 *
 * @author Peddi Swapna
 * @version 1.0
 * @since 20 Dec 2013
 */
public class MSReportDefinition extends ReportDefinition {
	
	/** The series names. */
	private String[] seriesNames;
	
	/** The is single line series result. */
	private Boolean isSingleLineSeriesResult=Boolean.FALSE;

	/**
	 * Gets the series names.
	 *
	 * @return the series names
	 */
	public String[] getSeriesNames() {
		return seriesNames;
	}
	
	/**
	 * Sets the series names.
	 *
	 * @param seriesNames the new series names
	 */
	public void setSeriesNames(String seriesNames[]) {
		this.seriesNames = seriesNames;
	}
	
	/**
	 * Sets the series names.
	 *
	 * @param seriesNames the new series names
	 */
	public void setSeriesNames(String seriesNames) {
		this.seriesNames = seriesNames.split(",");
	}
	
	/**
	 * Gets the checks if is single line series result.
	 *
	 * @return the checks if is single line series result
	 */
	public Boolean getIsSingleLineSeriesResult() {
		return isSingleLineSeriesResult;
	}
	
	/**
	 * Sets the checks if is single line series result.
	 *
	 * @param isSingleLineSeriesResult the new checks if is single line series result
	 */
	public void setIsSingleLineSeriesResult(Boolean isSingleLineSeriesResult) {
		this.isSingleLineSeriesResult = isSingleLineSeriesResult;
	}
	

}
