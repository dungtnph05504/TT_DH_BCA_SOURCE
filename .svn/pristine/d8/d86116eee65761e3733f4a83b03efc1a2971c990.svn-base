<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@ taglib prefix="form" uri="http://www.springframework.org/tags/form"%>
<%@ taglib prefix="spring" uri="http://www.springframework.org/tags"%>
<%@ taglib prefix="tiles" uri="http://tiles.apache.org/tags-tiles"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>
<%@ taglib uri="http://displaytag.sf.net" prefix="display"%>
<style>

</style>

<div id="fmBaoHuyHC">
    <div class="col-lg-12 mar-bottom-20" style="overflow-y:scroll; height:450px">
       <div class="col-sm-12 none-padding">
           <div class="content-item-title">
               <div class="oplog-title__txt">Hộ chiếu cần hủy</div>
           </div>
       </div>
       <div class="col-sm-12">
           <div class="col-sm-8">
               <div class="form-horizontal form-profile">
                   <div class="form-group">
                       <label class="col-sm-3 control-label text-right">Họ tên</label>
                       <div class="col-sm-9">
                           <input type="text" id="lostHoTen" class="form-control" value="${root.hoTen}">
                       </div>
                   </div>
                   <div class="form-group">
                       <label class="col-sm-3 control-label text-right">Ngày sinh</label>
                       <div class="col-sm-9">
                           <input type="text" id="lostNgaySinh" class="form-control" value="${root.ngaySinh}">
                       </div>
                   </div>
                   <div class="form-group">
                       <label class="col-sm-3 control-label text-right">Số CMND/CCCD</label>
                       <div class="col-sm-9">
                           <input type="text" id="lostCMND" class="form-control" value="${root.cmnd}">
                       </div>
                   </div>
                   <div class="form-group">
                       <label class="col-sm-3 control-label text-right">Số hộ chiếu</label>
                       <div class="col-sm-9">
                           <input type="text" id="lostSoHC" class="form-control" value="${root.soHC}">
                       </div>
                   </div>
                   <div class="form-group">
                       <label class="col-sm-3 control-label text-right">Ngày cấp HC</label>
                       <div class="col-sm-9">
                           <input type="text" id="lostNgayCap" class="form-control" value="${root.ngayCapHC}">
                       </div>
                   </div>
                   <div class="form-group">
                       <label class="col-sm-3 control-label text-right">Hạn HC</label>
                       <div class="col-sm-9">
                           <input type="text" id="lostHanHC" class="form-control" value="${root.hanHC}">
                       </div>
                   </div>
                   <div class="form-group">
                       <label class="col-sm-3 control-label text-right">Nơi cấp</label>
                       <div class="col-sm-9">
                           <input type="text" id="lostNoiCap" class="form-control" value="${root.noiCap}">
                       </div>
                   </div>
                   <div class="form-group">
                       <label class="col-sm-3 control-label text-right">Ngày đơn báo mất</label>
                       <div class="col-sm-9">
                           <input type="text" id="lostNgayBaoMat" class="form-control" value="">
                       </div>
                   </div>
                   <div class="form-group">
                       <label class="col-sm-3 control-label text-right">Hủy</label>
                       <div class="col-sm-9">
                           <input type="checkbox">
                       </div>
                   </div>
                   <div class="form-group">
                       <label class="col-sm-3 control-label text-right">In thông báo</label>
                       <div class="col-sm-9">
                           <input type="checkbox">
                       </div>
                   </div>
               </div>
           </div>
           <div class="col-sm-4">
               <div class="epp-right">
                   <div id="dialog-image_photoCompare" style="display: block;width: 135px;height: 180px;margin-top: 10px;margin-left: 5px;">
						<div class="centerCaption">       
					         <div style="text-align:center;">	
					         							             									                 
								    <c:choose>
					                    <c:when test="${not empty root.anhMat}">
					                        <div id="coAnhMat">
					                            <img src="data:image/jpg;base64,${root.anhMat}"
					                                 class="img-border doGetAJpgSafeVersion" height="180" width="135" />
					
					                        </div>
					                    </c:when>
					                    <c:otherwise>
					                        <div>
					                      
					                            <img src="<c:url value='/resources/images/No_Image.jpg' />" class="img-border" height="180" width="135" title="Tham chiếu" />										                           
					                        </div>
					                    </c:otherwise>
					                </c:choose>									       					                       										                  								                    									               
					         </div>								        
					    </div>
				    </div>
               </div>
           </div>
       </div>
       <div class="col-sm-12">
           <div class="form-group">
               <div class="col-sm-12 text-center">
                   <div>
                       <label class="e-font-bold" style="color:red">Số lượng hồ sơ cần hủy ${root.soLuongHS}</label>
                   </div>
               </div>
           </div>
       </div>
   </div>
</div>	
					      
		
