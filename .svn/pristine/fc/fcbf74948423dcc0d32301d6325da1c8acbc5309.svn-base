package com.nec.asia.nic.investigation.dto;

public class RequestTestApiDto {
	
	private String typeAPI;
	private String href;
	private String input;
	private String param;
	
	public RequestTestApiDto() {
		super();
	}
	
	public RequestTestApiDto(String typeAPI, String href, String input, String param) {
		super();
		this.typeAPI = typeAPI;
		this.href = href;
		this.input = input;
		this.param = param;
	}
	public String getTypeAPI() {
		return typeAPI;
	}

	public void setTypeAPI(String typeAPI) {
		this.typeAPI = typeAPI;
	}

	public String getHref() {
		return href;
	}

	public void setHref(String href) {
		this.href = href;
	}

	public String getInput() {
		return input;
	}

	public void setInput(String input) {
		this.input = input;
	}

	public String getParam() {
		return param;
	}

	public void setParam(String param) {
		this.param = param;
	}
	
	
}
