package com.nec.asia.nic.comp.job.command.exception;

import com.nec.asia.nic.framework.exception.CoreException;

public class NicCommandException extends CoreException{
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	/**
	 * Instantiates a new NicCommandException exception.
	 *
	 * @param message the message
	 */
	public NicCommandException(String message) {
		super(message);
		
	}

	/**
	 * Instantiates a new NicCommandException exception.
	 *
	 * @param cause the cause
	 */
	public NicCommandException(Throwable cause) {
		super(cause);
		
	}

	/**
	 * Instantiates a new NicCommandException exception.
	 *
	 * @param message the message
	 * @param cause the cause
	 */
	public NicCommandException(String message, Throwable cause) {
		super(message, cause);
		
	}
	


}
