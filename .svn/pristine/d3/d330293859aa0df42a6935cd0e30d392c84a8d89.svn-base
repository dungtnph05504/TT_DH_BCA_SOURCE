package com.nec.asia.nic.investigation.dto;

public class ConfirmLostDto {
	private String hoTen;
	private String ngaySinh;
	private String cmnd;
	private String soHC;
	private String ngayCapHC;
	private String hanHC;
	private String noiCap;
	private String ngayBaoMat;
	private int soLuongHS;
	private String anhMat;
	
	public ConfirmLostDto() {
		super();
	}
	
	public ConfirmLostDto(String hoTen, String ngaySinh, String cmnd,
			String soHC, String ngayCapHC, String hanHC, String noiCap,
			String ngayBaoMat) {
		super();
		this.hoTen = hoTen;
		this.ngaySinh = ngaySinh;
		this.cmnd = cmnd;
		this.soHC = soHC;
		this.ngayCapHC = ngayCapHC;
		this.hanHC = hanHC;
		this.noiCap = noiCap;
		this.ngayBaoMat = ngayBaoMat;
	}
	
	

	public int getSoLuongHS() {
		return soLuongHS;
	}

	public void setSoLuongHS(int soLuongHS) {
		this.soLuongHS = soLuongHS;
	}

	public String getAnhMat() {
		return anhMat;
	}

	public void setAnhMat(String anhMat) {
		this.anhMat = anhMat;
	}

	public String getHoTen() {
		return hoTen;
	}
	public void setHoTen(String hoTen) {
		this.hoTen = hoTen;
	}
	public String getNgaySinh() {
		return ngaySinh;
	}
	public void setNgaySinh(String ngaySinh) {
		this.ngaySinh = ngaySinh;
	}
	public String getCmnd() {
		return cmnd;
	}
	public void setCmnd(String cmnd) {
		this.cmnd = cmnd;
	}
	public String getSoHC() {
		return soHC;
	}
	public void setSoHC(String soHC) {
		this.soHC = soHC;
	}
	public String getNgayCapHC() {
		return ngayCapHC;
	}
	public void setNgayCapHC(String ngayCapHC) {
		this.ngayCapHC = ngayCapHC;
	}
	public String getHanHC() {
		return hanHC;
	}
	public void setHanHC(String hanHC) {
		this.hanHC = hanHC;
	}
	public String getNoiCap() {
		return noiCap;
	}
	public void setNoiCap(String noiCap) {
		this.noiCap = noiCap;
	}
	public String getNgayBaoMat() {
		return ngayBaoMat;
	}
	public void setNgayBaoMat(String ngayBaoMat) {
		this.ngayBaoMat = ngayBaoMat;
	}
	
}
