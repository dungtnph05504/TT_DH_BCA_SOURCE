package com.nec.asia.nic.comp.trans.dto;

import java.util.List;

public class NicSearchHitResultHitInfoItem {

	private String item;
	private List<NicSearchHitResultHitInfoItem> subItems;

	public NicSearchHitResultHitInfoItem(String item, List<NicSearchHitResultHitInfoItem> subItems) {
		super();
		this.item = item;
		this.subItems = subItems;
	}

	public String getItem() {
		return item;
	}

	public void setItem(String item) {
		this.item = item;
	}

	public List<NicSearchHitResultHitInfoItem> getSubItems() {
		return subItems;
	}

	public void setSubItems(List<NicSearchHitResultHitInfoItem> subItems) {
		this.subItems = subItems;
	}

}
