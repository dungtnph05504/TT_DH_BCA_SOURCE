package com.nec.asia.nic.comp.trans.service.exception;

import com.nec.asia.nic.framework.exception.CoreException;

public class NicAfisDataServiceException extends CoreException{

	/**
	 * Instantiates a new NicAfisDataServiceException exception.
	 *
	 * @param message the message
	 */
	public NicAfisDataServiceException(String message) {
		super(message);
		
	}
	
	
	/**
	 * Instantiates a new NicAfisDataServiceException exception.
	 *
	 * @param cause the cause
	 */
	public NicAfisDataServiceException(Throwable cause) {
		super(cause);
		
	}

	/**
	 * Instantiates a new NicAfisDataServiceException exception.
	 *
	 * @param message the message
	 * @param cause the cause
	 */
	public NicAfisDataServiceException(String message, Throwable cause) {
		super(message, cause);
		
	}
	
	
}
