package com.nec.asia.nic.framework.security.ad.exception;

import com.nec.asia.nic.framework.exception.CoreException;



/**
 * @author setia_budiyono
 *
 */
public class ActiveDirectoryServiceException extends CoreException {
	
	
	/**
	 * The Constructor.
	 */
	public ActiveDirectoryServiceException() {
		super("LDAP Service exception.");
	}

	/**
	 * The Constructor.
	 * 
	 * @param message
	 *            the message
	 */
	public ActiveDirectoryServiceException(String message) {
		super(message);
	}

	/**
	 * The Constructor.
	 * 
	 * @param message
	 *            the message
	 * @param cause
	 *            the cause
	 */
	public ActiveDirectoryServiceException(String message, Throwable cause) {
		super(message, cause);
	}

	/**
	 * The Constructor.
	 * 
	 * @param cause
	 *            the cause
	 */
	public ActiveDirectoryServiceException(Throwable cause) {
		super("AD Service Exception: "+cause.getMessage(),cause);
	}
}
