package com.nec.asia.nic.comp.trans.service.exception;

import com.nec.asia.nic.framework.exception.CoreException;

/**
 * @author setia_budiyono
 *
 */
public class DocumentDataServiceException extends CoreException {
	
	/**
	 * Instantiates a new DocumentDataServiceException exception.
	 *
	 * @param message the message
	 */
	public DocumentDataServiceException(String message) {
		super(message);
		
	}

	/**
	 * Instantiates a new DocumentDataServiceException exception.
	 *
	 * @param cause the cause
	 */
	public DocumentDataServiceException(Throwable cause) {
		super(cause);
		
	}

	/**
	 * Instantiates a new DocumentDataServiceException exception.
	 *
	 * @param message the message
	 * @param cause the cause
	 */
	public DocumentDataServiceException(String message, Throwable cause) {
		super(message, cause);
		
	}
}
