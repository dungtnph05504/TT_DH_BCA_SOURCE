package com.nec.asia.nic.dataSyncMonitoring;

import java.math.BigDecimal;

public class DataSyncMainInfo{
	
	private String applicateDate;
	
	private BigDecimal syncronizedTrans = new BigDecimal("0");
	
	private BigDecimal totTrans = new BigDecimal("0");
	
	private BigDecimal notRequired = new BigDecimal("0");
	
	private BigDecimal toBeSyncronized = new BigDecimal("0");
	
	private String status = "N";

	/**
	 * @return the applicateDate
	 */
	public String getApplicateDate() {
		return applicateDate;
	}

	/**
	 * @param applicateDate the applicateDate to set
	 */
	public void setApplicateDate(String applicateDate) {
		this.applicateDate = applicateDate;
	}

	/**
	 * @return the syncronizedTrans
	 */
	public BigDecimal getSyncronizedTrans() {
		return syncronizedTrans;
	}

	/**
	 * @param syncronizedTrans the syncronizedTrans to set
	 */
	public void setSyncronizedTrans(BigDecimal syncronizedTrans) {
		this.syncronizedTrans = syncronizedTrans;
	}

	/**
	 * @return the totTrans
	 */
	public BigDecimal getTotTrans() {
		return totTrans;
	}

	/**
	 * @param totTrans the totTrans to set
	 */
	public void setTotTrans(BigDecimal totTrans) {
		this.totTrans = totTrans;
	}

	/**
	 * @return the notRequired
	 */
	public BigDecimal getNotRequired() {
		return notRequired;
	}

	/**
	 * @param notRequired the notRequired to set
	 */
	public void setNotRequired(BigDecimal notRequired) {
		this.notRequired = notRequired;
	}

	/**
	 * @return the status
	 */
	public String getStatus() {
		return status;
	}

	/**
	 * @param status the status to set
	 */
	public void setStatus(String status) {
		this.status = status;
	}

	/**
	 * @return the toBeSyncronized
	 */
	public BigDecimal getToBeSyncronized() {
		return toBeSyncronized;
	}

	/**
	 * @param toBeSyncronized the toBeSyncronized to set
	 */
	public void setToBeSyncronized(BigDecimal toBeSyncronized) {
		this.toBeSyncronized = toBeSyncronized;
	}
	
	
}