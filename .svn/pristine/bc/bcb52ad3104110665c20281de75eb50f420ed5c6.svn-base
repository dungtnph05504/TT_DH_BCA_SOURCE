package com.nec.asia.nic.comp.trans.domain;

public class StatusticalIfor {
	   private Long id;
	   private String loaiHangDoi;
	   private int done;
	   private int pending;
	   private int doing;
	   private int none;
	   private int total;
	   
	public Long getId() {
		return id;
	}
	
	public StatusticalIfor(String loaiHangDoi, int done, int pending,
			int doing, int none, int total) {
		super();
		this.loaiHangDoi = loaiHangDoi;
		this.done = done;
		this.pending = pending;
		this.doing = doing;
		this.none = none;
		this.total = total;
	}

	public void setId(Long id) {
		this.id = id;
	}
	public String getLoaiHangDoi() {
		return loaiHangDoi;
	}
	public void setLoaiHangDoi(String loaiHangDoi) {
		this.loaiHangDoi = loaiHangDoi;
	}
	public int getDone() {
		return done;
	}
	public void setDone(int done) {
		this.done = done;
	}
	public int getPending() {
		return pending;
	}
	public void setPending(int pending) {
		this.pending = pending;
	}
	public int getDoing() {
		return doing;
	}
	public void setDoing(int doing) {
		this.doing = doing;
	}
	public int getNone() {
		return none;
	}
	public void setNone(int none) {
		this.none = none;
	}
	public int getTotal() {
		return total;
	}
	public void setTotal(int total) {
		this.total = total;
	}
	   
} 
