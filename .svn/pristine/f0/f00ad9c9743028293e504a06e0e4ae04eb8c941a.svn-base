package com.nec.asia.nic.comp.trans.service.exception;

import com.nec.asia.nic.framework.exception.CoreException;

/**
 * @author chris_wong
 *
 */
public class NicMainServiceException extends CoreException {
	
	/**
	 * Instantiates a new NicMainServiceException exception.
	 *
	 * @param message the message
	 */
	public NicMainServiceException(String message) {
		super(message);
		
	}

	/**
	 * Instantiates a new NicMainServiceException exception.
	 *
	 * @param cause the cause
	 */
	public NicMainServiceException(Throwable cause) {
		super(cause);
		
	}

	/**
	 * Instantiates a new NicMainServiceException exception.
	 *
	 * @param message the message
	 * @param cause the cause
	 */
	public NicMainServiceException(String message, Throwable cause) {
		super(message, cause);
		
	}
}
