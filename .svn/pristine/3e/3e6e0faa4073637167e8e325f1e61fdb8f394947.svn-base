package com.nec.asia.nic.comp.trans.dto;

public class DetailHandoverDto {
	private String siteId;
	private String sitePerso;
	private Long idPerso;
	private String code;
	private boolean stagePrint;
	private Long id;
	
	public DetailHandoverDto(String siteId, boolean stagePrint) {
		super();
		this.siteId = siteId;
		this.stagePrint = stagePrint;
	}
	public DetailHandoverDto() {
		super();
	}
	public String getSiteId() {
		return siteId;
	}
	public void setSiteId(String siteId) {
		this.siteId = siteId;
	}
	public boolean isStagePrint() {
		return stagePrint;
	}
	public void setStagePrint(boolean stagePrint) {
		this.stagePrint = stagePrint;
	}
	public String getSitePerso() {
		return sitePerso;
	}
	public void setSitePerso(String sitePerso) {
		this.sitePerso = sitePerso;
	}
	public Long getIdPerso() {
		return idPerso;
	}
	public void setIdPerso(Long idPerso) {
		this.idPerso = idPerso;
	}
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	
	
}
